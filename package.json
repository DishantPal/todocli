{
  "name": "todo-cli-app",
  "version": "1.0.0",
  "description": "A simple CLI Todo application",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "dev": "tsc --watch",
    "db:gen": "kysely-codegen --out-file ./src/db.d.ts",
    "migrate:latest": "node dist/migrations/migrator.js latest",
    "migrate:up": "node dist/migrations/migrator.js up",
    "migrate:down": "node dist/migrations/migrator.js down"
  },
  "keywords": [
    "todo",
    "cli",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.8.7",
    "kysely-codegen": "^0.17.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "kysely": "^0.26.3",
    "mysql2": "^3.6.2"
  }
}
